{
    "manifest_version": 2,
    "name": "Fahrenheit to Celcius",
    "version": "0.1",
    "background": {
        "scripts": ["background.js"]
      },
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["content.js"]
        }
      ],
      "browser_action": {
        "default_icon": "fahrenheit-icon.jpg"
      }

}